# Variables de entorno para Dataset Manager
# Copie este archivo como .env y configure los valores apropiados

# Base de datos MySQL (Solo lectura) - REQUERIDO
DB_PROD_RO_HOST=your_mysql_host
DB_PROD_RO_USER=your_readonly_user
DB_PROD_RO_PASSWORD=your_readonly_password
DB_PROD_RO_DATABASE=your_database_name
DB_PROD_RO_PORT=3306

# Elasticsearch (usar variables de entorno referenciadas en credentials_elastic_prod.json)
ELASTIC_HOST=your-elastic-host.eu-west-3.aws.elastic-cloud.com
ELASTIC_PORT=9243
ELASTIC_USERNAME=your_elastic_user
ELASTIC_PASSWORD=your_elastic_password
ELASTIC_API_KEY_ID=your_api_key_id
ELASTIC_API_KEY_SECRET=your_api_key_secret_base64

# Configuraci√≥n opcional
LOG_LEVEL=INFO
